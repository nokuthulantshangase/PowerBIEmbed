<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <img src="~/Content/logo-Gen.png" />
    </div>
    <div class="container body-content">
        <div class="tab-pane nav-divider show" style="background-color:antiquewhite">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" style="background-color:blue" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                </div>
            </div>
            <div class="navbar-collapse collapse">            
                <ul class="nav nav-tabs nav-justified" style="align-items:center">
                    <li class="active" style="background-color:cadetblue">
                        <a href="http://localhost:53898/">
                            <span class="glyphicon glyphicon-home"></span>
                            <span class="hidden-xs">Home</span>
                        </a>
                    </li>
                    <li>
                        <a href="http://localhost:54337/Default.aspx">
                            <span class="glyphicon glyphicon-dashboard"></span>
                            <span class="hidden-xs">Embed DashBoards</span>
                        </a>
                    </li>
                    <li>
                        <a href="http://localhost:53909/Default.aspx">
                            <span class="glyphicon glyphicon-stats"></span>
                            <span class="hidden-xs">Embed Reports</span>
                        </a>
                    </li>
                </ul>
            </div>
        </div>
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
